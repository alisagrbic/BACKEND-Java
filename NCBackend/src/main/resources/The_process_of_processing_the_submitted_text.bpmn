    <bpmn:userTask id="Task_1mwlb4h" name="List of reviewers" camunda:assignee="${EditorOfMagazine}">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="reviewer" label="Reviewer:" type="enum" />
        </camunda:formData>
        <camunda:taskListener event="create">
          <camunda:script scriptFormat="Groovy">taskFormData = task.getExecution().getProcessEngineServices().getFormService().getTaskFormData(task.getId());
rews = task.getExecution().getProcessEngineServices().getIdentityService().createUserQuery().memberOfGroup("reviewer").list();

formFields = taskFormData. getFormFields();
if(formFields!=null){
   for(field in formFields){
       if( field.getId() == "Reviewer"){
           //ovo je nase select polje
           items = field.getType().getValues();
           for(reviewer in rews){
               items.put(reviewer.getId(),reviewer.getFirstName()+" "+reviewer.getLastName());
           }
       }
   }
}</camunda:script>
        </camunda:taskListener>
        <camunda:taskListener event="complete">
          <camunda:script scriptFormat="Groovy">taskFormData = task.getExecution().getProcessEngineServices().getFormService().getTaskFormData(task.getId());
reviewerIds = new java.util.ArrayList();

formFields = taskFormData. getFormFields();
if(formFields!=null){
   for(field in formFields){
       if(field.getId() == "Reviewer"){
           if(field.getValue()!="none")
                reviewerIds.add(field.getValue());
       }
   }
}

task.getExecution().setVariable("reviewerList", reviewerIds);</camunda:script>
        </camunda:taskListener>
      </bpmn:extensionElements>
